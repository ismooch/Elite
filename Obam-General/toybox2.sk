command /magic [<text>]:
	trigger:
		player is in "world":
			if {db_effectshop.effect.%player%} is true:
				if argument is "default":
					if {effects.magic.%player%} is 0:
						set {effects.magic.%player%} to 1
						send "&fYou are now &cunstable&f." to player
						make player execute command "/magic effect"
						stop
					if {effects.magic.%player%} is 1:
						set {effects.magic.%player%} to 0
						send "&fYou are now &astable&f." to player
						stop
				if argument is "effect":
					if {effects.magic.%player%} is 1:
						set {_magic.loc.%player%} to location of player
						spawn 100 of red dust data 1 offset with 1, 1, 1 at {_magic.loc.%player%}
						wait 2 seconds
						make player execute command "/magic effect"
			if {db_effectshop.effect.%player%} is false:
				send "&eSorry, You have not unlocked this yet." to player
				stop
			
command /boat:
	trigger:
		player is in "world":
			if {toybox.boat.%player%} is 0:
				if {cartstop.%player%} is true:
					send "You are currently riding a minecart. Please exit the vehicle to use another vehicle."
					stop
				if player has {@gFirework} in his inventory:
					remove all {@gFirework} from player's inventory
					set {toybox.firework.%player%} to 0
				if player has {@gPiston} in his inventory:
					remove all {@gPiston} from player's inventory
					set {toybox.piston.%player%} to 0
				if player has {@gCart} in his inventory:
					remove all {@gCart} from player's inventory
					set {toybox.cart.%player%} to 0
				if {boatstop.%player%} is true:
					send "You already are using a boat." to player
					stop
				give 1 {@tempslot1} to player
				give 1 {@tempslot2} to player
				give 1 {@tempslot3} to player
				give 1 {@gBoat} to player
				remove all {@tempslot1} from player's inventory
				remove all {@tempslot2} from player's inventory
				remove all {@tempslot3} from player's inventory
				set {toybox.boat.%player%} to 1
				set {boatstop.%player%} to true
				send "You have received a boat." to player
				stop
			if {toybox.boat.%player%} is 1:
				if {boatstop.%player%} is true:
					delete vehicle of player
					set {boatstop.%player%} to false
				if player has {@gBoat} in his inventory:
					remove all {@gBoat} from player's inventory
					set {toybox.boat.%player%} to 0
					set {boatstop.%player%} to false
					stop
					
